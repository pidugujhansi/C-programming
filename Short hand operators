#include<stdio.h>
int main()
{
    int a,b;
    a=10,b=8;
    a+=3; //short hand operators
    printf("%d\n",a);
    a-=4;
    printf("%d\n",a);
    a*=2;
    printf("%d\n",a);
    a/=2;
    printf("%d\n",a);
    b+=2;
    printf("%d\n",b);
    b-=3;
    printf("%d\n",b);
    b*=2;
    printf("%d\n",b);
    b/=4;
    printf("%d",b);
    return 0;
}
